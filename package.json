{
	"name": "project-doryani",
	"version": "0.1.0",
	"description": "An overlay for Path of Exile 2.",
	"main": "out-tsc/electron/electron/main.js",
	"type": "commonjs",
	"private": true,
	"scripts": {
		"build:dev": "npm run build -- -c development",
		"build:prod": "npm run build -- -c production",
		"build": "npm run electron:serve-tsc && ng build",
		"electron:serve-tsc": "tsc -p tsconfig.electron.json",
		"electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
		"ng:lint": "ng lint",
		"ng:lint:fix": "ng lint --fix",
		"ng:serve": "ng serve",
		"ng:serve:dev": "ng serve -c development",
		"postinstall": "electron-builder install-app-deps",
		"start": "set NODE_OPTIONS=--max-old-space-size=7168 && npm-run-all -p ng:serve:dev electron:serve"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.0.5",
		"@angular/cli": "^19.0.5",
		"@angular/compiler-cli": "^19.0.0",
		"@stylistic/eslint-plugin": "^2.12.1",
		"@types/node": "^22.10.2",
		"angular-eslint": "19.0.2",
		"electron": "^33.2.1",
		"electron-builder": "^25.1.8",
		"electron-reloader": "^1.2.3",
		"eslint": "^9.16.0",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.9.2",
		"typescript": "~5.6.2",
		"typescript-eslint": "8.18.0",
		"wait-on": "^8.0.1"
	},
	"dependencies": {
		"@angular/animations": "^19.0.0",
		"@angular/cdk": "^19.0.4",
		"@angular/common": "^19.0.0",
		"@angular/compiler": "^19.0.0",
		"@angular/core": "^19.0.0",
		"@angular/forms": "^19.0.0",
		"@angular/material": "^19.0.4",
		"@angular/platform-browser": "^19.0.0",
		"@angular/platform-browser-dynamic": "^19.0.0",
		"@angular/router": "^19.0.0",
		"@ngx-translate/core": "^16.0.3",
		"electron-log": "^5.2.4",
		"get-windows": "^9.2.0",
		"localforage": "^1.10.0",
		"ngx-color-picker": "^17.0.0",
		"node-window-manager": "^2.2.4",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"uiohook-napi": "^1.5.4",
		"zone.js": "~0.15.0"
	}
}
